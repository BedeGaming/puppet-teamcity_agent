[Unit]
Description=<%= @service %>

[Service]
Type=simple
User=<%= @user %>
Group=<%= @user %>
ExecStart=/home/<%= @user %>/buildAgent/bin/agent.sh "start"
RemainAfterExit=yes
Restart=always
PIDFile=/home/<%= @user %>/buildAgent/logs/buildAgent.pid
KillSignal=SIGKILL

[Install]
WantedBy=multi-user.target
